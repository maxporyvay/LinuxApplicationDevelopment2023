#: ../src/bisect.c:23
msgid "# Bisect Guess Game\n-f   Use roman numbers instead of decimal numbers"
msgstr "# Bisect Guess Game\n-f   Использовать римские цифры вместо десятичных"

#: ../src/bisect.c:79
#, c-format
msgid "Guess a number from %d to %d\n"
msgstr "Загадайте число от %d до %d\n"

#: ../src/bisect.c:86
#, c-format
msgid "Is the number greater than %s? Enter yes or no\n"
msgstr "Число больше %s? Введите да или нет\n"

#: ../src/bisect.c:88
#, c-format
msgid "Is the number greater than %d? Enter yes or no\n"
msgstr "Число больше %d? Введите да или нет\n"

#: ../src/bisect.c:91
msgid "yes"
msgstr "да"

#: ../src/bisect.c:93
msgid "no"
msgstr "нет"

#: ../src/bisect.c:97
#, c-format
msgid "I think that your number is %d!\n"
msgstr "Кажется, Ваше число равно %d!\n"
